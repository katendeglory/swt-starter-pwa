{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "tailwind:watch": "postcss public/tailwind.css -o public/index.css -w",
    "tailwind:build": "cross-env NODE_ENV=production postcss public/tailwind.css -o public/index.css",
    "dev:only": "cross-env HOST=0.0.0.0 PORT=80 rollup -c -w",
    "build": "yarn run tailwind:build && rollup -c && yarn run injectManifest",
    "start": "sirv public",
    "serve": "serve public -p 80",
    "dev": "cross-env HOST=0.0.0.0 PORT=80 concurrently \"yarn run dev:only\" \"yarn run tailwind:watch\"",
    "git:push": "git add . && git commit -am \"Yet another commit\" && git push origin main",
    "git:deploy": "yarn run export --basepath svelte-tailwind-starter && gh-pages -d public",
    "web:build": "yarn run git:push && yarn run build && Xcopy /E /I /Y .\\public .\\..\\public",
    "web:deploy": "yarn run git:push && yarn run build && Xcopy /E /I /Y .\\public .\\..\\public && cd ../public && yarn run git:push && cd ../client",
    "surge:deploy": "yarn run build && surge public https://781227.surge.sh/",
    "surge:teardown": "surge teardown https://781227.surge.sh/",
    "generateSW": "C:\\Users\\hp\\AppData\\Local\\Yarn\\bin\\workbox.cmd generateSW",
    "injectManifest": "C:\\Users\\hp\\AppData\\Local\\Yarn\\bin\\workbox.cmd injectManifest workbox-config.js",
    "onchange": "onchange 'src/**/*.*' -- yarn run injectManifest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@tailwindcss/forms": "^0.2.1",
    "autoprefixer": "^9",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "dependencies": {
    "@beyonk/svelte-carousel": "^2.8.0",
    "add": "^2.0.6",
    "axios": "^0.21.0",
    "graphql": "^15.3.0",
    "graphql-request": "^3.0.0",
    "lodash.get": "^4.4.2",
    "sirv-cli": "^1.0.8",
    "svelte-loading-spinners": "^0.1.1",
    "svelte-scrollto": "^0.2.0",
    "svelte-select": "^3.11.1",
    "svelte-spa-router": "^3.0.5",
    "yarn": "^1.22.10"
  }
}